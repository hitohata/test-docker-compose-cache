{
  "group": {
    "default": {
      "targets": [
        "cache-build"
      ]
    }
  },
  "target": {
    "cache-build": {
      "context": ".",
      "dockerfile": "dockerfile/Dockerfile",
      "cache-from": [
        "type=local,src=/tmp/cache/.buildx-cache"
      ],
      "cache-to": [
        "type=local,dest=/tmp/out/.buildx-cache"
      ],
      "output": [
        "type=local,dest=/tmp/out/.buildx-cache"
      ]
    }
  }
}
