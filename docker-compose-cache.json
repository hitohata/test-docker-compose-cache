{
  "group": {
    "default": {
      "targets": [
        "cache-build"
      ]
    }
  },
  "target": {
    "cache-build": {
      "context": ".",
      "dockerfile": "dockerfile/Dockerfile",
      "cache-from": [
        "type=local,dist=/tmp/.buildx-cache"
      ],
      "cache-to": [
        "type=local,src=/tmp/.buildx-cache"
      ],
      "output": [
        "type=local"
      ]
    }
  }
}
